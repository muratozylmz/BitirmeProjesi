@{
    ViewData["Title"] = "Buybox";
}

@section Styles {
    <link rel="stylesheet" href="~/css/reports/buybox.css">
}


    <div class="content-section dark-theme">
        <!-- Page Header -->
        <div class="header-section">
            <div class="title-area">
                <h1>Buybox Dağılımı</h1>
            </div>
            <div class="header-actions">
                <a href="#" class="youtube-link">
                    <img src="youtube-icon.svg" alt="YouTube">
                </a>
                <div class="country-selector">
                    <img src="uae-flag.svg" alt="UAE flag">
                    <span>U.A.E.</span>
                    <i class="material-icons">arrow_drop_down</i>
                </div>
            </div>
        </div>

        <!-- Info Box -->
        <div class="info-box">
            <p>Aynı listelerde satış yaptığınız satıcıların envanterinizdeki buybox dağılımını görün.</p>
        </div>

        <!-- Search Section -->
        <div class="search-container">
            <i class="material-icons">search</i>
            <input type="text" placeholder="Ara: Seller Id">
        </div>

        <!-- Buybox Table -->
        <div class="buybox-table">
            <table>
                <thead>
                    <tr>
                        <th>SELLER ID</th>
                        <th>ÜRÜN SAYISI</th>
                        <th>SİPARİŞ SAYISI</th>
                        <th>AKSİYON</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>A2QUTRSO1ZHRN9</td>
                        <td>1.444</td>
                        <td>1</td>
                        <td><i class="material-icons">more_vert</i></td>
                    </tr>
                    <tr>
                        <td>A1DR0UBUFGJSQ</td>
                        <td>434</td>
                        <td>1</td>
                        <td><i class="material-icons">more_vert</i></td>
                    </tr>
                    <tr>
                        <td>A10LE8TF2RTIHL</td>
                        <td>388</td>
                        <td>1</td>
                        <td><i class="material-icons">more_vert</i></td>
                    </tr>
                    <tr>
                        <td>A2M3I7Y6VFH2XN</td>
                        <td>239</td>
                        <td>1</td>
                        <td><i class="material-icons">more_vert</i></td>
                    </tr>
                    <tr>
                        <td>AKY1SMQBO3GSQ</td>
                        <td>130</td>
                        <td>0</td>
                        <td><i class="material-icons">more_vert</i></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
@section Scripts {
    <script src="~/js/reports/buybox.js"></script>
}